#include <iostream>

using namespace std;

int main(){
    int numero;
    int x;
    int cont;
    cout << "Ingrese un numero: " << endl;
    cin>>numero;
    while(cont==0 && x<numero){
            if(numero%x==0)
                cont=cont+1;
            x=x+1;
            }
    if(cont==0)
        cout <<"el numero_"<< numero <<"_es primo"<<endl;
    else
        cout <<"el numero_"<< numero <<"_no es primo"<<endl;

    return 0;
}
